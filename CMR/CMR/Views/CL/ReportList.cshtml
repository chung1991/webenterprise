@model IEnumerable<CMR.Models.AnnualCourseRecord>

<h4>Your Report List</h4>
<table class="table">
    <tr>
        <th>
            @Html.DisplayName("Course Name")
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AnnualCourse.academicYear)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ApproveStatu.name)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.AnnualCourse.Course.name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.AnnualCourse.academicYear)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ApproveStatu.name)
        </td>
        <td>
            @Html.ActionLink("Edit", "EditReport", new { id=item.annualCourseRecordId }) |
            @Html.ActionLink("Details", "ReportDetail", new { id=item.annualCourseRecordId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.annualCourseRecordId })
        </td>
    </tr>
}

</table>
